{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "jsx": "react",
    "declaration": true,
    "outDir": "../workspace-ui/lib",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "../workspace-ui",
    "paths": {
      "@/*": ["../workspace-ui/*"],
      "@/library/*": ["../workspace-ui/library/*"],
      "@/components/*": ["../workspace-ui/components/*"]
    }
  },
  "include": ["../workspace-ui/components", "../workspace-ui/library", "../workspace-ui/*.ts"],
  "exclude": ["../workspace-ui/node_modules"]
}
  